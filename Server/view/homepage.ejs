<!-- scroll up button -->
<button id="back-to-top-btn"><i class="fas fa-angle-double-up"></i></button>

<!-- Intro carousel -->
<div
  id="MagicCarousel"
  class="carousel slide transparent-bgd"
  data-ride="carousel"
>
  <ol class="carousel-indicators">
    <li data-target="#MagicCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#MagicCarousel" data-slide-to="1"></li>
    <li data-target="#MagicCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner" role="listbox">
    <div class="carousel-item active">
      <img
        class="d-block w-100 transparent-bgd"
        src="./img/woman_coffee.jpg"
        alt="First Slide"
      />
      <div class="carousel-caption">
        <h3>Welcome to Beanie Coffee Shop</h3>
        <p>We are here to serve you the finest coffee you ever tasted</p>
      </div>
    </div>
    <div class="carousel-item">
      <img
        class="d-block w-100 transparent-bgd"
        src="./img/cfmachine.jpg"
        alt="Second Slide"
      />
      <div class="carousel-caption">
        <h3>Welcome to Beanie Coffee Shop</h3>
        <p>We are here to serve you the finest coffee you ever tasted</p>
      </div>
    </div>
    <div class="carousel-item">
      <img
        class="d-block w-100 transparent-bgd"
        src="./img/intro.jpg"
        alt="Third Slide"
      />
      <div class="carousel-caption">
        <h3>Welcome to Beanie Coffee Shop</h3>
        <p>We are here to serve you the finest coffee you ever tasted</p>
      </div>
    </div>
    <a
      class="carousel-control-prev"
      href="#MagicCarousel"
      role="button"
      data-slide="prev"
    >
      <span class="carousel-control-prev-icon"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a
      class="carousel-control-next"
      href="#MagicCarousel"
      role="button"
      data-slide="next"
    >
      <span class="carousel-control-next-icon"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>


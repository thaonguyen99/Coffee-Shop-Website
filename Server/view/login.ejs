<%-include("partials/header")-%> <%# CONTENT GOES HERE %>

<style>
  body {
    background: url(img/bg-img3.jpg) no-repeat;
    background-size: cover;
    width: 100%;
    height: 100%;
  }
</style>
<div class="login-box">
  <%if(success===false){%> <%=msg%> <%}%>
  <div class="login-container">
    <form class="login-form" method="POST" action="/login">
      <h1 style="font-family: 'Allura', cursive;">Welcome</h1>
      <div class="text-box one">
        <div class="login-i">
          <i class="fa fa-user"></i>
        </div>
        <div>
          <h5>Username</h5>
          <input class="input" type="username" name="username" />
        </div>
      </div>

      <div class="text-box two">
        <div class="login-i">
          <i class="fa fa-lock"></i>
        </div>
        <div>
          <h5>Password</h5>
          <input class="input" type="password" name="password" />
        </div>
      </div>
      <input class="login-btn" type="submit" value="Login" />
    </form>
  </div>
</div>

<script src="javascript/login-effect.js"></script>
<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
